{
  "name": "@creditiq/web-base",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "preversion": "npm run test",
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "postcompile": "node_modules/.bin/babel dist -d dist --source-maps inline",
    "compile": "node_modules/.bin/tsc -p .",
    "compile:watch": "node_modules/watch/cli.js 'npm run compile' src",
    "watch": "npm-run-all --parallel test:watch compile:watch",
    "start": "npm run dev",
    "dev": "npm-run-all --parallel open:src",
    "dev:offline": "npm-run-all --parallel 'open:src -- --offline' test:watch",
    "dev:demo": "npm-run-all --parallel 'open:src -- demo' test:watch",
    "dev:api": "npm-run-all --parallel 'open:src -- api' test:watch",
    "dev:staging": "npm-run-all --parallel 'open:src -- staging' test:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "pretest": "npm run compile",
    "test": "jest --config jest.single.config.js",
    "test:watch": "CI=true jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creditiq/web-base.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/mockery": "^1.4.29",
    "@types/node": "^7.0.12",
    "@types/sinon": "^1.16.31",
    "@types/sinon-as-promised": "^4.0.4",
    "autoprefixer": "~6.4.0",
    "awesome-typescript-loader": "^3.1.3",
    "babel-cli": "^6.18.0",
    "babel-core": "~6.24.0",
    "babel-jest": "^20.0.0",
    "babel-loader": "~6.2.5",
    "babel-plugin-react-display-name": "~2.0.0",
    "babel-plugin-transform-react-constant-elements": "~6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "~0.2.9",
    "babel-polyfill": "~6.16.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-preset-stage-1": "~6.13.0",
    "babel-register": "~6.14.0",
    "browser-sync": "~2.14.0",
    "chalk": "~1.1.3",
    "convert-source-map": "1.3.0",
    "copy-webpack-plugin": "~3.0.1",
    "core-js-webpack-plugin": "~1.0.3",
    "css-loader": "~0.24.0",
    "ejs-compiled-loader": "~1.1.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.9.0",
    "html-webpack-plugin": "~2.22.0",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "jest-cli": "^21.1.0",
    "mockery": "^1.4.0",
    "node-sass": "~3.8.0",
    "npm-run-all": "^4.1.1",
    "sass-loader": "~4.1.1",
    "sinon": "^1.17.2",
    "sinon-as-promised": "^4.0.0",
    "source-map-loader": "~0.2.0",
    "style-loader": "~0.13.1",
    "tap-spec": "^4.1.1",
    "transform-ts-paths": "^1.0.0",
    "ts-jest": "^20.0.11",
    "tslint": "^5.4.2",
    "typescript": "^2.3.4",
    "validate-commit-msg": "^2.14.0",
    "watch": "^0.17.1",
    "webpack": "~1.14.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-fail-plugin": "~1.0.5",
    "webpack-hot-middleware": "~2.12.2",
    "webpack-md5-hash": "~0.0.5"
