    "start": "node dist/index.js",
    "dev": "npm run watch",
    "pretest": "npm run compile",
    "test": "jest --config jest.single.config.js",
    "test:watch": "CI=true jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creditiq/base.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/mockery": "^1.4.29",
    "@types/node": "^7.0.12",
    "@types/sinon": "^1.16.31",
    "@types/sinon-as-promised": "^4.0.4",
    "babel-cli": "^6.18.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015-node4": "^2.1.0",
    "convert-source-map": "1.3.0",
    "husky": "^0.14.3",
    "jest": "^21.0.1",
    "jest-cli": "^21.1.0",
    "mockery": "^1.4.0",
    "npm-run-all": "^4.1.1",
    "sinon": "^1.17.2",
    "sinon-as-promised": "^4.0.0",
    "tap-spec": "^4.1.1",
    "transform-ts-paths": "^1.0.0",
    "ts-jest": "^20.0.11",
    "tslint": "^5.4.2",
    "typescript": "^2.3.4",
    "validate-commit-msg": "^2.14.0",
    "watch": "^0.17.1"
  },
  "engines": {
    "node": ">=4.3.x"
  },
